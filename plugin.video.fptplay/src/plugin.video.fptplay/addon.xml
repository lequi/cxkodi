<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.fptplay" name="Fpt Play" version="1.5.1" provider-name="repository.cx.com">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.xbmcswift2" version="2.4.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.service"
			library="start.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language></language>
    <summary lang="vi">Xem video trực tuyến</summary>
    <description lang="vi">Ứng dụng cho phép xem video trực tuyến, nguồn video từ website fpt play</description>
  </extension>
</addon>
